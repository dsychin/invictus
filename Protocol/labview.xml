<?xml version="1.0"?>
<PropertyList>
    <generic>
        <output>
            <line_separator>newline</line_separator>
            <var_separator>,</var_separator>
            <chunk>
                <node>/instrumentation/airspeed-indicator/indicated-speed-kt</node>
                <name>Airspeed</name>
                <type>float</type>
                <format>%07.3f</format>
            </chunk>
            <chunk>
                <node>/instrumentation/altimeter/indicated-altitude-ft</node>
                <name>altitude</name>
                <type>float</type>
                <format>%09.3f</format>
            </chunk>
            <chunk>
                <node>/instrumentation/vertical-speed-indicator/indicated-speed-kts</node>
                <name>vertical speed</name>
                <type>float</type>
                <format>%07.3f</format>
            </chunk>
			<chunk>
                <node>/instrumentation/attitude-indicator/indicated-pitch-deg</node>
                <name>Pitch deg</name>
                <type>float</type>
                <format>%07.3f</format>
            </chunk>
			<chunk>
                <node>/instrumentation/attitude-indicator/indicated-roll-deg</node>
                <name>Roll deg</name>
                <type>float</type>
                <format>%07.3f</format>
            </chunk>
			<chunk>
                <node>/instrumentation/heading-indicator/indicated-heading-deg</node>
                <name>Heading deg</name>
                <type>float</type>
                <format>%07.3f</format>
            </chunk>
			<chunk>
                <node>/instrumentation/turn-indicator/indicated-turn-rate</node>
                <name>Turn indicator</name>
                <type>float</type>
                <format>%06.3f</format>
            </chunk>
			<chunk>
                <node>/position/latitude-deg</node>
                <name>Latitude</name>
                <type>float</type>
                <format>%018.14f</format>
            </chunk>
            <chunk>
                <node>/position/longitude-deg</node>
                <name>Longitude</name>
                <type>float</type>
                <format>%018.14f</format>
            </chunk>
        </output>
        <input>
            <line_separator>newline</line_separator>
            <var_separator>,</var_separator>
            <!-- Controls -->
            <chunk>
                <name>aileron</name>
                <type>float</type>
                <node>/controls/flight/aileron</node>
                <format>%f</format>
            </chunk>
            <chunk>
                <name>elevator</name>
                <type>float</type>
                <node>/controls/flight/elevator</node>
                <format>%f</format>
            </chunk>
            <chunk>
                <name>throttle</name>
                <type>float</type>
                <node>/controls/engines/engine/throttle</node>
                <format>%f</format>
            </chunk>
            <chunk>
                <name>rudder</name>
                <type>float</type>
                <node>/controls/flight/rudder</node>
                <format>%f</format>
            </chunk>
        </input>
    </generic>
</PropertyList>